# Global fallback
QWEN_MODEL=qwen-plus
OLLAMA_BASE_URL=http://localhost:11434
DASHSCOPE_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
DASHSCOPE_API_KEY=
OPENAI_API_KEY=
OPENAI_BASE_URL=

# Per-agent model routing
# PROVIDER: openai | ollama
# MODEL: model id for selected provider

# Supervisor: intent classify + redo decision
SUPERVISOR_PROVIDER=ollama
SUPERVISOR_MODEL=qwen-plus

# Tooler branches
TOOLER_TEXT_PROVIDER=ollama
TOOLER_TEXT_MODEL=qwen-plus
TOOLER_IMAGE_PROVIDER=ollama
TOOLER_IMAGE_MODEL=qwen-plus
TOOLER_MERGE_PROVIDER=ollama
TOOLER_MERGE_MODEL=qwen-plus

# Downstream agents
SEARCHER_PROVIDER=ollama
SEARCHER_MODEL=qwen-plus
PLANNER_PROVIDER=ollama
PLANNER_MODEL=qwen-plus
REFLECTOR_PROVIDER=ollama
REFLECTOR_MODEL=qwen-plus
SUMMARIZER_PROVIDER=ollama
SUMMARIZER_MODEL=qwen-plus

# Runtime
AGENT_LOG_TRUNCATE=600
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s | %(levelname)s | %(name)s | %(message)s
LOG_DIR=./logs
LOG_FILE_NAME=app.debug.log
LOG_FILE_WHEN=midnight
LOG_FILE_INTERVAL=1
LOG_FILE_BACKUP_COUNT=7
LOG_FILE_ENCODING=utf-8
LOG_FILE_LEVEL=DEBUG

# LangSmith tracing
LANGCHAIN_TRACING_V2=true
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=medgemma-afu

# Search tools
TAVILY_API_KEY=
CHROMA_DIR=./data/chroma
WEB_SEARCH_MAX_RESULTS=5
RAG_TOP_K=3

# Database
DB_PATH=./data/patients.db
